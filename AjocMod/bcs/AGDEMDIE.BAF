IF
	True()
THEN
	RESPONSE #100
		CutSceneId("agdeano")
		ForceSpell("agdeimos",WIZARD_DISINTEGRATE)
		Wait(2)
		ForceSpell("agdeimos",WIZARD_LIGHTNING_BOLT)
		Wait(2)
		ForceSpell("agdeimos",WIZARD_IMPRISONMENT)
		Wait(1)
		ActionOverride("agdeimos",DestroySelf())
		SetGlobal("deimos2","AG8000",1)
		EndCutSceneMode()
END

